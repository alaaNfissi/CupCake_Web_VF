<!DOCTYPE html>
<html lang="en">

<head>
    <title>
        {% block pageTitle %}
        {% endblock %}
    </title>
    {% block head %}
        <meta charset="utf-8">
        <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <!-- Favicons -->
        <link rel="apple-touch-icon" href="{{ asset('assets/img/kit/pro/apple-icon.png') }}">
        <link rel="icon"  href="{{ asset('assets/img/kit/pro/favicon.png') }}">
        <!--     Fonts and icons     -->
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
        {#<link rel="stylesheet" href="{{ asset('assets/css/material-kit.css?v=2.0.2') }}">#}
        <!-- Documentation extras -->
        <!-- CSS Just for demo purpose, don't include it in your project -->
        <link href="{{ asset('assets/assets-for-demo/demo.css') }}" rel="stylesheet" />
        <link href="{{ asset('assets/assets-for-demo/vertical-nav.css') }}" rel="stylesheet" />
    {% endblock %}
</head>

{% block body %}
    {% block pageClass %}
    {% endblock %}
    {% block navbar %}
        <!--         inverse navbar with notifications     -->
        <nav class="navbar navbar-inverse navbar-expand-lg bg-primary navbar-color-on-scroll navbar-transparent fixed-top " color-on-scroll="100" id="sectionsNav" role="navigation-demo">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-translate">
                    {#<a class="navbar-brand" href="#">Navbar with notification</a>#}
                    <form class="form-inline ml-auto">
                        <button type="submit" class="btn btn-white btn-raised btn-fab btn-round">
                            <i class="material-icons">search</i>
                        </button>
                        <div class="form-group has-white">
                            <input type="text" class="form-control" placeholder="Search">
                        </div>
                    </form>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                        <span class="navbar-toggler-icon"></span>
                        <span class="navbar-toggler-icon"></span>
                    </button>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                Discover
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                Wishlist
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="{{ path('user_homepage') }}" class="btn btn-rose btn-raised btn-fab btn-round">
                                <i class="material-icons">home</i>
                            </a>
                        </li>
                        <li class="dropdown nav-item">
                            <a href="#" class="btn btn-rose btn-raised btn-fab btn-round" data-toggle="dropdown">
                                <i class="material-icons">email</i>
                                <span class="notification">1</span>
                                {% block mailsnbr %}
                                {% endblock %}
                            </a>
                            {% block mails %}
                            {% endblock %}
                        </li>
                        <li class="nav-item">
                            <button class="btn btn-rose btn-raised btn-fab btn-round" data-toggle="modal" data-target="#signupModal">
                                <i class="material-icons">shopping_cart</i>
                            </button>
                        </li>
                        <li class="dropdown nav-item">
                            <a href="#" class="profile-photo dropdown-toggle nav-link" data-toggle="dropdown">
                                <div class="profile-photo-small">
                                    <img src="{{ app.user.image }}" alt="Circle Image" class="rounded-circle img-fluid">
                                </div>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right">
                                <h6 class="dropdown-header">Cup Cake</h6>
                                <a href="{{ path('fos_user_profile_show') }}" class="dropdown-item">Me</a>
                                {% if is_granted('ROLE_PATISSIER') %}
                                    <a href="{{ path('user_dashboard') }}" class="dropdown-item">My Dashboard</a>
                                    <a href="{{ path('patisserie_space') }}" class="dropdown-item">My Space</a>
                                {% endif %}
                                <a href="#" class="dropdown-item">Settings and other stuff</a>

                                {% if is_granted('ROLE_USER') %}
                                    <a href="{{ path('fos_user_security_logout') }}" class="dropdown-item">Sign out</a>
                                {% else %}
                                    <a href="{{ path('fos_user_security_login') }}" class="dropdown-item">Sign in</a>
                                {% endif %}
                            </div>
                        </li>
                        <li>
                            <span>{{ app.user.prenom }}</span>
                        </li>
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container-->
        </nav>
    {% endblock %}

    {% block main %}
    {% endblock %}
    {#<button type="button" class="btn btn-primary" data-toggle="modal" data-target=".bd-example-modal-lg">Large modal</button>#}
    {% block footer %}
        <footer class="footer ">
            <div class="container">
                <nav class="pull-left">
                    <ul>
                        <li>
                            <a href="https://www.creative-tim.com">
                                Creative Tim
                            </a>
                        </li>
                        <li>
                            <a href="http://presentation.creative-tim.com">
                                About Us
                            </a>
                        </li>
                        <li>
                            <a href="http://blog.creative-tim.com">
                                Blog
                            </a>
                        </li>
                        <li>
                            <a href="https://www.creative-tim.com/license">
                                Licenses
                            </a>
                        </li>
                    </ul>
                </nav>
                <div class="copyright pull-right">
                    &copy;
                    <script>
                        document.write(new Date().getFullYear())
                    </script>, made with <i class="material-icons">favorite</i> by
                    <a href="https://www.creative-tim.com" target="_blank">Creative Tim</a> for a better web.
                </div>
            </div>
        </footer>
    {% endblock %}
    {% block scripts %}

    <!--   Core JS Files   -->
    <script src="{{ asset('assets/js/core/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/js/core/popper.min.js') }}"></script>
    <script src="{{ asset('assets/js/bootstrap-material-design.js') }}"></script>
    <!--  Google Maps Plugin  -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBNduoRB6rgeXnHvjbppiJphh5p-KpT8Jc"></script>
    <!--  Plugin for Date Tim   e Picker and Full Calendar Plugin  -->
    <script src="{{ asset('assets/js/plugins/moment.min.js') }}"></script>
    <!--	Plugin for the Datepicker, full documentation here: https://github.com/Eonasdan/bootstrap-datetimepicker -->
    <script src="{{ asset('assets/js/plugins/bootstrap-datetimepicker.min.js') }}"></script>
    <!--	Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
    <script src="{{ asset('assets/js/plugins/nouislider.min.js') }}"></script>
    <!--	Plugin for Select, full documentation here: http://silviomoreto.github.io/bootstrap-select -->
    <script src="{{ asset('assets/js/plugins/bootstrap-selectpicker.js') }}"></script>
    <!--	Plugin for Tags, full documentation here: http://xoxco.com/projects/code/tagsinput/  -->
    <script src="{{ asset('assets/js/plugins/bootstrap-tagsinput.js') }}"></script>
    <!--	Plugin for Fileupload, full documentation here: http://www.jasny.net/bootstrap/javascript/#fileinput -->
    <script src="{{ asset('assets/js/plugins/jasny-bootstrap.min.js') }}"></script>
    <!--	Plugin for Small Gallery in Product Page -->
    <script src="{{ asset('assets/js/plugins/jquery.flexisel.js') }}"></script>
    <!-- Plugins for presentation and navigation  -->
    <script src="{{ asset('assets/assets-for-demo/js/modernizr.js') }}"></script>
    <script src="{{ asset('assets/assets-for-demo/js/vertical-nav.js') }}"></script>
    <!-- Material Kit Core initialisations of plugins and Bootstrap Material Design Library -->
    <script src="{{ asset('assets/js/material-kit.js?v=2.0.2') }}"></script>
    <!-- Fixed Sidebar Nav - js With initialisations For Demo Purpose, Don't Include it in your project -->
    <script src="{{ asset('assets/assets-for-demo/js/material-kit-demo.js') }}"></script>
    {% endblock %}
{% endblock %}

</html>
